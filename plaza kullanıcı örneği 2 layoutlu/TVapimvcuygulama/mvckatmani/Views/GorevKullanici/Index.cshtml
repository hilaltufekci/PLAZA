@model IEnumerable<mvckatmani.Models.mvcGorevModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "EY")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.GörevTanimi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Vardiya)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.GörevTanimi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Vardiya)
        </td>
        <td>
            @Html.ActionLink("Edit", "EY", new {  id=item.GörevNo}) |
            @Html.ActionLink("Delete", "Delete", new { id = item.GörevNo })
        </td>
    </tr>
}

</table>
