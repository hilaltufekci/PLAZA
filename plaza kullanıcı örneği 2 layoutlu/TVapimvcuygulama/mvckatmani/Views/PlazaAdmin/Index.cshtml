@model IEnumerable<mvckatmani.Models.mvcPlazaModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "EY")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PlazaAdi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PlazaKatSayisi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Il)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ilce)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.PlazaAdi)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.PlazaKatSayisi)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Il)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Ilce)
    </td>
    <td>
        @Html.ActionLink("Edit", "EY", new {  id=item.PlazaNo }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.PlazaNo })
    </td>
</tr>
}

</table>
