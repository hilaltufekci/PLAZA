@model IEnumerable<TVapimvcuygulama.Models.Kullanicilar>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "SıgnUp")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.KullaniciAdi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Sifre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.KullaniciAdi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sifre)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.KullaniciNo }) |
            @Html.ActionLink("Details", "Details", new { id=item.KullaniciNo }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.KullaniciNo })
        </td>
    </tr>
}

</table>
